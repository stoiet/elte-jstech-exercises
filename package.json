{
  "name": "elte-jstech-exercises",
  "version": "1.0.0",
  "description": "ELTE JavaScript Technologies Exercises",
  "scripts": {
    "clean": "rm -rf dist/* 2> /dev/null",
    "create-dist": "test -d dist/ || mkdir -p dist/",
    "prebundle": "npm run clean && npm run create-dist",
    "example": "babel-node --presets latest",
    "exercise00": "mocha exercise00/*.js --opts .mocharc",
    "exercise01": "mocha exercise01/*.js --opts .mocharc",
    "exercise02": "mocha exercise02/*.js --opts .mocharc",
    "exercise03": "mocha exercise03/*.js --opts .mocharc",
    "exercise04": "mocha exercise04/*.js --opts .mocharc",
    "exercise05": "mocha exercise05/*.js --opts .mocharc",
    "exercise06": "babel-node --presets latest ./exercise06/*.js",
    "exercise07": "babel-node --presets latest ./exercise07/*.js",
    "exercise08": "mocha exercise08/*.js --opts .mocharc",
    "exercise09": "mocha exercise09/*.js --opts .mocharc --require co-mocha",
    "exercise10": "npm run prebundle && cp exercise10/index.html dist/ && webpack --config exercise10/webpack.config.js --watch",
    "exercise11": "npm run prebundle && cp exercise11/index.html dist/ && webpack --config exercise11/webpack.config.js --watch",
    "exercise12": "npm run prebundle && cp exercise12/index.html dist/ && webpack --config exercise12/webpack.config.js --watch",
    "exercise13": "nodemon --watch exercise13 exercise13/index.js",
    "exercise14": "nodemon --watch exercise14 exercise14/index.js",
    "exercise15-client-build": "npm run prebundle && webpack --config exercise15/client/webpack.config.js --watch",
    "exercise15-client-server": "node exercise15/client-server.js",
    "exercise15-server": "nodemon --watch exercise15/server exercise15/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoiet/elte-jstech-exercises.git"
  },
  "author": "stoiet",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/stoiet/elte-jstech-exercises/issues"
  },
  "homepage": "https://github.com/stoiet/elte-jstech-exercises#readme",
  "dependencies": {
    "angular": "1.6.0",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "co-mocha": "1.1.3",
    "codemirror": "5.20.0",
    "core-js": "2.4.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader": "0.4.4",
    "json-loader": "0.5.4",
    "jsonwebtoken": "7.1.9",
    "koa": "1.2.4",
    "koa-bodyparser": "2.3.0",
    "koa-router": "5.4.0",
    "koa-views": "4.1.0",
    "lodash": "4.16.4",
    "mocha": "3.1.2",
    "mongodb": "2.2.14",
    "nodemon": "1.11.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "static-server": "2.0.3",
    "style-loader": "0.13.1",
    "webpack": "1.13.2"
  }
}
